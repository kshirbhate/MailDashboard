
@{
    ViewBag.Title = "SentMail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm())
{
    <div class="main">
        <div class="container">
            <div id="loginbox" class="col-md-12  col-sm-12">
                <h2>Mail Box</h2>
                <hr />

                <div class="col-lg-1 col-md-1">
                    <div class="sidenav">
                        <a href='@Url.Action("Index", "Mailbox", new { area = "Mailbox"})'>Inbox</a>
                        <a href='@Url.Action("SentMail", "Mailbox", new { area = "Mailbox"})'>Sent</a>
                        <a href='@Url.Action("DraftMail", "Mailbox", new { area = "Mailbox"})'>Drafts</a>
                        <a href='@Url.Action("SpamMail", "Mailbox", new { area = "Mailbox"})'>Spam</a>

                    </div>
                </div>                

                <div class="col-lg-11 col-md-11">
                    <div class="mailbox-sub-title" id="mailbox-sub-title">@Model.MailBoxSubTitle</div>
                    <table id="table-data-collection-pagination" class="table table-striped table-bordered" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th id="first-header-column">From</th>
                                <th>Subjet</th>
                                <th>Time</th>
                            </tr>
                        </thead>

                        <tbody>
                            <tr>
                                <td>Tushar</td>
                                <td>On Leave</td>
                                <td>27/01/2017</td>
                            </tr>
                            <tr>
                                <td>Chaitanya</td>
                                <td>Working From Home</td>
                                <td>26/01/2017</td>
                            </tr>
                            <tr>
                                <td>Aman</td>
                                <td>Session on Payroll</td>
                                <td>25/01/2017</td>
                            </tr>
                            @*@for (int i = 0; i < Model.List.Count; i++)
                                {
                                    <tr>
                                        <td>@Model.List[i].PrintId</td>
                                        <td>@Model.List[i].UserRealName</td>
                                        <td>@Model.List[i].CategoryName</td>
                                        <td><a href='@Url.Action("Edit", "Print", new { PrintId = Model.List[i].PrintId , area = "System"})'>Edit</a></td>
                                    </tr>
                                }*@
                        </tbody>
                    </table>
                </div>



            </div>

            <div class="clearfix"></div>
        </div>

    </div>

}

